#include <iostream>
#include <vector>
using namespace std;
class Solution {
public:
    vector<vector<int>> imageSmoother(vector<vector<int>>& M) {
        int row=M.size();
        int col=M[0].size();
        for(int i=0;i<row;i++)
        {
            for(int j=0;j<col;j++)
            {
                
            }
        }
    }
};
int main() {
    
    
    Solution sol;
    sol.imageSmoother();
   
}
